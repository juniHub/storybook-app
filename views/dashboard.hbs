<div id="public-stories">
    <button class="dashboard-btn"><a href="/stories">Visit All Public Stories Here</a></button>
</div>

<h3>Welcome {{name}} 😊</h3>
<h5>Your stories shown here or create new one ❤️</h5>
<h6>Click on <a href="/stories/add" class="btn-floating btn-large waves-effect waves-light purple lighten-1"><i
            class="fas fa-plus"></i></a> to create new
    one!</h6>

{{#condition success.length ">" 0}}

<div class="materialert success">
    <div class="material-icons"><i class="fa fa-check"></i></div>
    <span>{{success}}</span>
    <button type="button" class="close-alert">×</button>
</div>

{{/condition}}


<div id="search-stories">

    <div class="row">



        <div class="row">

            <h3>#Filter Tags</h3>

            <div id=facet-list></div>

        </div>

        <div class="row">

            <h3>⭐All Your Public Stories</h3>

            <ul id=container>
            </ul>

        </div>

    </div>

</div>

<div id="dashboard-stories">
    <button class="dashboard-btn"><a href="/stories/dashboardStory">Manage Your Public Stories</a></button>

    <button class="dashboard-btn"><a href="/stories/privateStory">Manage Your Private Stories</a></button>

</div>